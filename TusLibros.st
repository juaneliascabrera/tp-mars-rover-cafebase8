!classDefinition: #Carrito category: #TusLibros!
Object subclass: #Carrito
	instanceVariableNames: 'catalog products'
	classVariableNames: ''
	poolDictionaries: ''
	category: 'TusLibros'!

!Carrito methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:53:34'!
add: aProduct
	(catalog includes: aProduct) ifTrue:[^products add: aProduct].
	^self error: 'El producto ingresado no está en el catálogo.'! !

!Carrito methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 22:09:47'!
add: aProduct times: aNumber
	(catalog includes: aProduct) ifTrue:[aNumber timesRepeat: [products add: aProduct]]
	ifFalse: [self error: 'El producto ingresado no está en el catálogo.']! !

!Carrito methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:54:53'!
includes: aProduct
	^true! !

!Carrito methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 22:08:56'!
includes: aProduct times: aNumber
	^ (products occurrencesOf: aProduct) = aNumber! !

!Carrito methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:53:45'!
initializeFor: aCatalog
	catalog := aCatalog.
	products := OrderedCollection new.! !

!Carrito methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:34:56'!
isEmpty
	^true.! !

