	category: 'TusLibros-Test'!

!CarritoTest methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:34:34'!
test01AlCrearElCarritoEstaVacio
	|carrito|
	carrito := Carrito new.
	self assert: carrito isEmpty.! !

!CarritoTest methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:54:16'!
test02NoSePuedeAgregarUnProductoQueNoEsteEnElCatalogo
	|carrito catalogo|
	catalogo := OrderedCollection new.
	catalogo add: 'Harry Potter y la Piedra Filosofal'.
	
	carrito := Carrito for: catalogo.
	
	self should: [carrito add: 'La Divina Comedia'] raise: Error withMessageText: 'El producto ingresado no está en el catálogo.'! !

!CarritoTest methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 21:50:58'!
test03SePuedeAgregarUnProductoQueEsteEnElCatalogo
	|carrito catalogo|
	catalogo := OrderedCollection new.
	catalogo add: 'Harry Potter y la Piedra Filosofal'.
	
	carrito := Carrito for: catalogo.
	carrito add: 'Harry Potter y la Piedra Filosofal'.
	
	self assert: carrito includes: 'Harry Potter y la Piedra Filosofal'.! !

!CarritoTest methodsFor: 'as yet unclassified' stamp: 'JC 11/7/2025 22:09:11'!
test04SePuedeAgregarVariasVecesUnProductoQueEsteEnElCatalogo
	|carrito catalogo|
	catalogo := OrderedCollection new.
	catalogo add: 'Harry Potter y la Piedra Filosofal'.
	
	carrito := Carrito for: catalogo.
	carrito add: 'Harry Potter y la Piedra Filosofal' times: 4.
	
	self assert: (carrito includes: 'Harry Potter y la Piedra Filosofal' times: 4).! !
