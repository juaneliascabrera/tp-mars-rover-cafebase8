!classDefinition: #TestsMarsRover category: #'tp-MarsRover'!
TestCase subclass: #TestsMarsRover
	instanceVariableNames: ''
	classVariableNames: ''
	poolDictionaries: ''
	category: 'tp-MarsRover'!

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:32:11'!
test01GivenTheEmptyStringTheRoverShouldNotMove
	|aRover|
	aRover := MarsRover new.
	
	aRover process: ''.
	
	self assert: aRover isAt equals: 0@0.
	
	! !

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:31:56'!
test02GivenTheForwardCommandTheRoverShouldMoveForward
	|aRover|
	aRover := MarsRover new.
	
	aRover process: 'f'.
	
	self assert: aRover isAt equals: 0@1.
	
	! !

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:36:40'!
test03GivenTheBackwardCommandTheRoverShouldMoveBackward
	|aRover|
	aRover := MarsRover new.
	
	aRover process: 'b'.
	
	self assert: aRover isAt equals: 0@-1.
	
	! !

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:40:08'!
test04GivenTheForwardCommandTwiceTheRoverShouldMoveForwardTwice
	|aRover|
	aRover := MarsRover new.
	
	aRover process: 'f'.
	aRover process: 'f'.
	
 	self assert: aRover isAt equals: 0@2.
	
	! !

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:47:43'!
test05GivenTheBackwardCommandTwiceTheRoverShouldMoveBackwardTwice
	|aRover|
	aRover := MarsRover new.
	
	aRover process: 'b'.
	aRover process: 'b'.
	
 	self assert: aRover isAt equals: 0@-2.
	
	! !

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:52:29'!
test06GivenTheEmptyStringTheRoverShouldNotRotate
	|aRover|
	aRover := MarsRover new.
	
	aRover process: ''.
	
	self assert: aRover looksAt equals: $N.
	
	! !

!TestsMarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 22:02:36'!
test07GivenTheLeftRotateCommandTheRoverShouldRotateLeft
	|aRover|
	aRover := MarsRover new.
	self assert: aRover looksAt equals: $N.
	aRover process: 'l'.
	self assert: aRover looksAt equals: $O.
	
	! !


!classDefinition: #MarsRover category: #'tp-MarsRover'!
Object subclass: #MarsRover
	instanceVariableNames: 'currentPosition currentCardinalPoint'
	classVariableNames: ''
	poolDictionaries: ''
	category: 'tp-MarsRover'!

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 22:01:40'!
initialize
	currentPosition := 0@0.
	currentCardinalPoint  := $N.! !

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:33:47'!
isAt
	^currentPosition.! !

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 22:02:10'!
looksAt
	^currentCardinalPoint.! !

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:45:22'!
moveBackward
	currentPosition := currentPosition + (0@-1).! !

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 21:45:12'!
moveForward
	currentPosition := currentPosition + (0@1).! !

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 22:00:57'!
process: aString
	aString = 'f' ifTrue: [self moveForward].
	aString = 'b' ifTrue: [self moveBackward].	
	aString = 'l' ifTrue: [self rotateLeft].	
	! !

!MarsRover methodsFor: 'as yet unclassified' stamp: 'lv 10/17/2025 22:01:27'!
rotateLeft
	currentCardinalPoint := $O.! !
